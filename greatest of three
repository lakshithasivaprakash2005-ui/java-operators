import java.util.Scanner;
public class main{
    public static void main(String[]args){
        Scanner sc=new Scanner (System.in);
        int a=1;
        int b=2;
        int c=3;
        if(a>b)
           System.out.println(a);
        else if(b>c)
           System.out.println(b);
        else 
            System.out.println(c);
    }
}