import java.util.Scanner;
class Q19 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num1 = sc.nextInt();
        int num2 = sc.nextInt();
        int product = num1 * num2;
        if (product > 0)
            System.out.println("Positive");
        else if (product < 0)
            System.out.println("Negative");
        else
            System.out.println("Zero");
    }
}
