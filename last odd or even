import java.util.Scanner;
class Q12 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int lastDigit = num % 10;
        if (lastDigit % 2 == 0)
            System.out.println("Even");
        else
            System.out.println("Odd");
    }
}
